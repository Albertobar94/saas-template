{
  "name": "@repo/eslint-plugin-eslint",
  "private": true,
  "sideEffects": false,
  "version": "0.1.0",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsup",
    "clean": "git clean -xdf .turbo node_modules .next .cache .vitest tsconfig.tsbuildinfo",
    "lint": "eslint src/ --ext .ts,.tsx",
