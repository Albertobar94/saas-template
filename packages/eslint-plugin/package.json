    "postinstall": "pnpm build"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^5.62.0",
    "@typescript-eslint/utils": "^5.62.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@repo/config": "workspace:^",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.12.11",
    "tsup": "^8.0.2"
  }
}
